sudo cp /home/min/.builds/linux-4.1-samus/firmware/* /lib/firmware
sudo cp /home/min/.builds/linux-4.1-samus/firmware/* /usr/lib/firmware
ALSA_CONFIG_UCM=/home/min/.builds/linux-4.1-samus/scripts/setup/ucm/ alsaucm -c bdw-rt5677 set _verb HiFi
sudo sed -i '/^[ 	]*load-module module-alsa-source device=hw:1,2/d' /etc/pulse/default.pa
sudo sed -i '/load-module module-udev-detect/ i load-module module-alsa-source device=hw:0,2' /etc/pulse/default.pa 
amixer -c0 set Master "100%"
amixer -c0 set Speaker toggle
amixer -c0 set Headphone toggle
amixer -c0 set Mic "100%"
